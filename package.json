{
  "name": "parcellab-challenge-greg-zander",
  "version": "1.0.0",
  "author": {
    "email": "grigorii.zander@gmail.com",
    "name": "Greg Zander"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:test": "pnpm db:test:reset && export NODE_ENV=\"test\" && pnpm next build",
    "start": "next start",
    "start:test": "export NODE_ENV=\"test\" && next start",
    "test:static": "pnpm lint && pnpm typecheck",
    "test:unit": "jest __tests__/ --passWithNoTests",
    "test": "pnpm test:static && pnpm test:unit",
    "lint": "next lint --no-cache",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force && prisma migrate deploy",
    "db:test:reset": "env-cmd -f .env.test pnpx prisma migrate reset --force && prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "cypress:open": "env-cmd -f .env.test pnpx cypress open",
    "cypress:start": "start-server-and-test 'pnpm start:test' 3000 'pnpm cypress:open'",
    "cypress:build": "pnpm build:test && pnpm cypress:start"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@paralleldrive/cuid2": "^2.2.1",
    "@prisma/client": "^4.16.1",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "formik": "^2.4.2",
    "jest-mock-extended": "^3.0.4",
    "next": "13.4.7",
    "next-themes": "^0.2.1",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "postcss-focus-visible": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-topbar-progress-indicator": "^4.1.1",
    "swr": "^2.2.0",
    "tailwindcss-radix": "^2.8.0",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.7",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.15.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.16.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
